generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id     Int      @id @default(autoincrement())
  email  String   @unique
  name   String
  birth  DateTime @db.Date
  gender String   @db.Char(1)
  image  String?
}
